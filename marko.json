{
  "<coffee-script>": {
    "code-generator": "./app/assets/tags/coffee-script",
    "body": "static-text",
    "preserve-whitespace": true,
    "@bare": "boolean"
  },
  "<container>": {
    "template": "./app/assets/tags/container.marko"
  },
  "<media-controls>": {
    "template": "./app/assets/tags/media-controls.marko"
  },
  "<stylus-css>": {
    "code-generator": "./app/assets/tags/stylus-css",
    "body": "static-text",
    "preserve-whitespace": true,
    "@compress": "boolean",
    "@paths": "array"
  }
}
