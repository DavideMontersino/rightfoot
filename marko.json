{
  "<banner-container>": {
    "template": "./lib/tags/banner-container.marko"
  },
  "<coffee-script>": {
    "code-generator": "./lib/tags/coffee-script",
    "body": "static-text",
    "preserve-whitespace": true,
    "@bare": "boolean"
  },
  "<media-controls>": {
    "template": "./lib/tags/media-controls.marko"
